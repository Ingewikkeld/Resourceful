<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ingewikkeld_rest_user.controller.user.class">Ingewikkeld\Rest\UserBundle\Controller\UserController</parameter>
    </parameters>

    <services>
        <service id="ingewikkeld_rest_user.controller.user" class="%ingewikkeld_rest_user.controller.user.class%">
            <argument type="service" id="translator" />
            <argument type="service" id="form.factory" />
            <argument type="service" id="fos_user.user_manager" />
            <argument type="service" id="router" />
        </service>
    </services>
</container>
